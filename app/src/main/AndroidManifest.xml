<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zhangqianyuan.teamwork.lostandfound">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/diai"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/diai"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".view.activity.MainActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".view.activity.LogInActivity"
            android:label="@string/title_activity_log_in"
            android:theme="@style/AppTheme" />
        <activity android:name=".view.activity.VerifyActivity" />
        <activity android:name=".view.activity.SignInActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.activity.ThingDetailActivity" />
        <activity
            android:name=".view.activity.UserInfoSettingActivity"
            android:label="@string/editInfo"
            android:theme="@style/AppTheme">
        </activity>
        <activity android:name=".view.activity.UploadActivity" />
        <activity android:name=".view.activity.UploadFormActivity" />
        <activity android:name=".view.activity.UserInfoAboutUsActivity"
            android:theme="@style/AppTheme"
            android:label="关于我们"
            />
        <activity android:name=".view.activity.UserInfoMyUpload"
            android:theme="@style/AppTheme"
            android:label="我的发布"
            />
    </application>

</manifest>